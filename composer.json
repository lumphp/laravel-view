{
    "name": "lumphp/laravel-view",
    "description": "The Illuminate View package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "lumphp/laravel-collections": "^8.0",
        "lumphp/laravel-container": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-events": "^8.0",
        "lumphp/laravel-filesystem": "^8.0",
        "lumphp/laravel-macroable": "^8.0",
        "lumphp/laravel-support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\View\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
